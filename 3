import numpy as np #транспонирование
a = np.array([[0, 1, 2], [3, 4, 5]])
print('A:', a)
a = a.transpose()
print(' транспонирование матрицы A:')
print(a)
print()

#умножение матриц
A = np.array([[2, -3, 1], [5, 4, -2]])
B = np.array([[-7, 5], [2, -1], [4, 3]])
print('A: ', A)
print('B: ', B)
print('умножение матриц AB:')
print(np.dot(A,B))

print()
#ранг матрицы
r=np.linalg.matrix_rank(A)
print('ранг матрицы A: ', r)
